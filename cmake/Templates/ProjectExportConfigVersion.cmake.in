set(PACKAGE_VERSION "@PACKAGE_VERSION@")

# Check whether the requested PACKAGE_FIND_VERSION is compatible
if("${PACKAGE_VERSION}" VERSION_LESS "${PACKAGE_FIND_VERSION}")
  set(PACKAGE_VERSION_COMPATIBLE FALSE)
else()
  set(PACKAGE_VERSION_COMPATIBLE TRUE)
  if ("${PACKAGE_VERSION}" VERSION_EQUAL "${PACKAGE_FIND_VERSION}")
    set(PACKAGE_VERSION_EXACT TRUE)
  endif()
endif()

set(@PROJECT_NAME@_VERSION ${PACKAGE_VERSION})
STRING(REPLACE "\." ";" _VERSION "${PACKAGE_VERSION}")
LIST(GET _VERSION 0 @PROJECT_NAME@_VERSION_MAJOR)
LIST(GET _VERSION 1 @PROJECT_NAME@_VERSION_MINOR)
LIST(GET _VERSION 2 @PROJECT_NAME@_VERSION_PATCH)

unset(_VERSION)
